extends layout

block content
    h1= title
    h2 Upload Photos
    form(method = 'POST', action = '/create', enctype = 'multipart/form-data')
      input(type = 'file', name = 'image', id = 'image')
      br
      input(type = 'text', name = 'title', id = 'title') 
      br
      input(type = 'submit', value = 'Share Photo')